    android:layout_height="match_parent"
    android:background="@drawable/subselect"
    tools:context="com.example.bhatt.selfhelpher.FirstWindows">

    <include
        android:id="@+id/custometool"
        layout="@layout/app_bar" />

    <FrameLayout
        android:id="@+id/sub_select_fragment1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
